<h1>Listing users</h1>

<ul id="user-list">
  <% if @users.count == 0 %>
    <p class="message">
      <%= _("There are no registered users in this system.") %>
    </p>
  <% end %>
  <% @users.each do |user| %>
<li><%= link_to user.full_name, user %> <%= _('of %s') % user.gaming_group if user.gaming_group %> </li>
<% end %>
</ul>
